{% block main %}
    <table>
        <tr>
            <th> Meeting Id </th>
            <th> Description </th>
            <th> Time </th>
            <th> Date </th>
            <th> Room </th>
            <th> Approval </th>
        </tr>

        <!--Collecting the list of meeting-->
        {% for meeting in meetings %}

            <tr>
                <td> {{ meeting.id }}</td>
                <td> {{ meeting.description }}</td>
                <td> {{ meeting.time }}</td>
                <td> {{ meeting.date }}</td>
                <td> {{ meeting.room }}</td>
                <td> {{ meeting.approval }}</td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}