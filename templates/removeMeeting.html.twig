{% extends '_base.html.twig' %}

{% block main %}
    <!--This is the log out the page-->
    <div id = "logout">
        <a href = "/killSession">Log Out</a>

        {% if message %}
            <p>{{ message }}</p>
        {% endif %}
    </div>
    <!--This is the delete the meeting-->
    <div id ="deleteMeeting">

        <h1>Remove Meeting</h1>

        <form action="/processRemoveMeeting" method="post">

        <span class = "form1">
        <label for = "meetingId">meetingId:</label>
            <input type="text" title = "meetingId" name= "id"  placeholder="Enter meeting Id must filled in" autofocus />
        </span>

        <span class = "form5">
            <input type="submit" value="Submit">
            <input type="reset" value="Reset">
        </span>
        </form>

    </div>
    <div id = "displayMeeting">
        {% include 'listMeeting.html.twig' %}
    </div>

{% endblock %}