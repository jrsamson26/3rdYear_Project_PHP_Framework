{% block main %}

<table>
    <tr>
        <th> Id </th>
        <th> Username </th>
        <th> Role </th>
    </tr>

    {% for student in students %}

        <tr>
            <td> {{ student.id }}</td>
            <td> {{ student.username }}</td>
            <td> {{ student.role }}</td>
        </tr>
    {% endfor %}
</table>

{% endblock %}