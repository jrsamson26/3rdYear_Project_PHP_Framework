{% extends '_base.html.twig' %}

<!--I tried to implement the project but it didn't work-->
{% block main %}
    <!--This is the log out the page-->
    <div id = "logout">
        <a href = "/killSession">Log Out</a>

        {% if message %}
            <p>{{ message }}</p>
        {% endif %}
    </div>

    <div id="">
        <h1>Add Project</h1>

        <form action="/processUpdateProject" method="post">

            <label for="title">title:</label>
            <input type="text" title="title" name="title" autofocus>


            <label for="description">description:</label>
            <textarea title="description" name="description"></textarea>


            <label for="members" >members:</label>
            <input type="text" title="members" name="members">


            <label for="supervisor">supervisor:</label>
            <input type="text" title="supervisor" name="supervisor">


            <label for="deadline">deadline:</label>
            <input type="date" title="deadline" name="deadline">


            <input type="submit" value="Submit">
            <input type="reset" value="Reset">

        </form>
    </div>
    <div id="displayProject">
        {% include 'projectsList.html.twig' %}
    </div>

{% endblock %}